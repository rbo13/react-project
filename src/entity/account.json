{
  "name": "Account",
  "columns": {
    "id": {
      "primary": true,
      "type": "int",
      "generated": true
    },
    "first_name": {
      "type": "varchar"
    },
    "last_name": {
      "type": "varchar"
    },
    "username": {
      "type": "varchar",
      "length": 25,
      "unique": true
    },
    "password": {
      "type": "text"
    },
    "salt": {
      "type": "text",
      "default": ""
    },
    "createdAt": {
      "type": "bigint",
      "default": 0
    },
    "updatedAt": {
      "type": "timestamp",
      "default": null
    },
    "deletedAt": {
      "type": "timestamp",
      "default": null
    }
  }
}
